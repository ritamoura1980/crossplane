# Exemplo de arquivo de secrets para DEV
# Copie este arquivo para secrets.yaml e ajuste os valores
# O arquivo secrets.yaml está no .gitignore e não será commitado

apiVersion: kustomize.config.k8s.io/v1beta1
kind: Kustomization

secretGenerator:
- name: app-secrets
  literals:
  - API_KEY=dev-key-not-secret
  - DB_PASSWORD=dev-password-not-secret
  type: Opaque
