# Exemplo de arquivo de secrets para STAGING
# Copie este arquivo para secrets.yaml e ajuste os valores
# O arquivo secrets.yaml está no .gitignore e não será commitado

apiVersion: kustomize.config.k8s.io/v1beta1
kind: Kustomization

secretGenerator:
- name: app-secrets
  literals:
  - API_KEY=CHANGE-ME-STAGING
  - DB_PASSWORD=CHANGE-ME-STAGING
  type: Opaque
